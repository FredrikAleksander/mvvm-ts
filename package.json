    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^2.35.0",
    "edgedriver": "^4.15063.0",
    "finalhandler": "^1.1.0",
    "geckodriver": "^1.10.0",
    "get-port": "^3.2.0",
    "mocha": "^5.0.0",
    "nightwatch": "^0.9.19",
    "rollup": "^0.55.3",
    "rollup-plugin-ejs": "^1.1.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-typescript2": "^0.11.0",
    "selenium-server": "^3.9.1",
